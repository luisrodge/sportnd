<% if Team.any? %>
  <div class="application-col">
      <div class="header">
      <div class="row">
        <div class="col-md-12">
          <h3>Enrolled Teams You Can Join (<%= teams_enrolled_count %>)</h3>
        </div>
      </div>
    </div>

      <div class="js-items-container">
      	<%= render partial: 'teams/team', collection: @teams %>
      </div>
      <div class="row">
        <div class="col-md-1 center-block pagination" data-pagination-endpoint="<%= @endpoint %>" data-pagination-pages="<%= @page_amount %>">
          <%= image_tag 'ajax-loader.gif' %>
        </div>
      </div>
  </div>
<% end %>
