  <div class="members-wrapper">
    <div class="row gutter-60">
      <div class="col-md-5">
        <h5 class="tournament-type"><%= link_to team.tournament.sport.name + " Tournament", tournament_path(team.tournament), class: "notice" %></h5>
        <div class="team-container <%= team.color.name.downcase %>">
          <h4><%= team.color.name %> Team</h4>
          <h5><%= team_space_txt(team) %></h5>
          <h5>Bet Entry @ <%= money_format(team.tournament.bet_amount) %></h5>
          <%= join_btn(team.tournament, team, "sm") %>
        </div>
      </div>
      <div class="col-md-7">
        <% team.users.each do |user| %>
          <div class="col-md-4">
            <h4 class="truncate"><%= user.full_name %></h4>
            <%= image_tag(user.profile_image_url(:medium), class: "img-circle") if user.profile_image? %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
