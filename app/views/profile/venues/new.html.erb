<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for [:profile, Venue.new] do |f| %>
			<div class="form-group">
				<%= f.text_field :name, class: "form-control", placeholder: "Enter venue name" %>
			</div>
			<div class="form-group">
				<%= f.text_field :address, class: "form-control", placeholder: "Enter address name" %>
			</div>
			<div class="form-group">
				<%= f.file_field :venue_image %>
			</div>
			<div class="form-group">
				<%= f.submit class: "btn btn-primary" %>
			</div>
		<% end %>
	</div>
</div>